generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model rol {
  id_rol   Int       @id @default(autoincrement())
  rol_name String    @unique(map: "Rol_rol_name_key")
  usuario  usuario[]
}

model usuario {
  id_user       Int     @id @default(autoincrement())
  user_name     String
  user_password String
  RolId         Int
  rol           rol     @relation(fields: [RolId], references: [id_rol], map: "Usuario_RolId_fkey")

  @@index([RolId], map: "Usuario_RolId_fkey")
}
